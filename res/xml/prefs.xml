<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <EditTextPreference
        android:title="Edit Box"
        android:key="name"
        android:summary="Enter Name">
    </EditTextPreference>
    
    <CheckBoxPreference
        android:title="Music"
        android:key="chkbox"
        android:summary="Start screen music"
        android:defaultValue="true"
        ></CheckBoxPreference>

    <ListPreference
        android:title="List title"
        android:key="list"
        android:summary="This is a a list"
        android:entries="@array/list"
        android:entryValues="@array/lValues"
        ></ListPreference>

</PreferenceScreen>
